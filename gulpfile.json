{
  "build": {
    "path": {
      "build-name": "isabase-app",
      "build-dir": "isabase/dist",
      "src-dir": "isabase/src",
      "app-dir": "isabase/src/app",
      "concat-output-dir": "isabase/dist/assets/js",
      "css-dir": "isabase/src/assets/css",
      "css-output-dir": "isabase/dist/assets/css",
      "components-dir": [
        "bower_components/jquery/dist/jquery.js",
        "bower_components/angular/angular.js",
        "bower_components/angular-resource/angular-resource.js",
        "bower_components/angular-ui-router/release/angular-ui-router.js",
        "bower_components/angular-translate/angular-translate.js",
        "bower_components/angular-translate-loader-static-files/angular-translate-loader-static-files.js",
        "bower_components/angular-mocks/angular-mocks.js",
        "bower_components/restful-ng-mock/build/restful-ng-mock.js",
        "bower_components/AngularJS-Toaster/toaster.js",
        "bower_components/angular-animate/angular-animate.js",
        "bower_components/angular-breadcrumb/dist/angular-breadcrumb.js",
        "bower_components/angular-sanitize/angular-sanitize.js",
        "bower_components/purl/purl.js",
        "semantic/dist/semantic.js"
      ],
      "themes-dir": "semantic/dist/themes",
      "themes-output-dir": "isabase/dist/assets/css/themes"
    },
    "clean": true,
    "default": true,
    "task-dir": "tasks/dist",
    "task": {
      "concat": true,
      "copy": true,
      "css": true,
      "themes": true
    }
  }
}
